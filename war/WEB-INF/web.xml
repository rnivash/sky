<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">   
    <servlet>
        <servlet-name>sign</servlet-name>
        <servlet-class>Users.Pages.LoginUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sign</servlet-name>
        <url-pattern>/sign</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AccountEngine</servlet-name>
        <servlet-class>Cash.Pages.AccountEngine</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccountEngine</servlet-name>
        <url-pattern>/Account</url-pattern>
    </servlet-mapping>
	<servlet>
        <servlet-name>ScriptsEngine</servlet-name>
        <servlet-class>Scripts.Pages.ScriptsEngine</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ScriptsEngine</servlet-name>
        <url-pattern>/scripts</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>	
</web-app>